<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.project.eatapp">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Board.InActivity">
        </activity>
        <activity android:name=".Chat.ChattingActivity">
        </activity>
        <activity android:name=".Board.MyReviewActivity">
        </activity>
        <activity android:name=".Board.EditorListActivity">
        </activity>
        <activity android:name=".LoginandMain.HomeActivity">
        </activity>
        <activity android:name=".LoginandMain.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>